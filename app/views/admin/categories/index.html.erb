<h2 class="page-header">Manage Categories <%= link_to "Create New", new_admin_category_path,
class: 'btn btn-primary pull-right'%></h2>
<table class="table table-striped">
  <tr>
    <th>Category Title</th>
    <th>Create Date</th>
    <th>Action</th>
  </tr>
  <% @categories.each do |category| %>
  <tr>
    <td><% category.name %></td>
    <td><%= category.created_at%>
    <td><%= link_to "Edit",edit_admin_category_path(category), class:'btn
    btn-primary' %><%= link_to "Delete", admin_category_path(category), class:
    'btn btn-danger', method: :delete, date: {confirm: 'Are you sure'}%></td>
  </tr>
  <% end %>
  </table>
